class Main {
    public static void main(String[] args) {
      int arr[][]={{1,2,3},{4,5,6},{7,8,9}};
      int primary =0,secondary=0;
      int n=arr.length;
      for(int i=0;i<n;i++){
          primary+=arr[i][i];
          secondary+=arr[i][n-i-1];
      }
      System.out.println(primary);
      System.out.println(secondary);
    }
}

